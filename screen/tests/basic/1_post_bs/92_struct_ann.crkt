%%TEST%%
the struct annotation
%%ARGS%%
%%FILE%%
import crack.io cerr;
@import crack.ann struct;

@struct Foo { String a; int b, c; }
f := Foo('test', 100, 200);
if (f.a != 'test')
    cerr `FAILED string field\n`;
if (f.b != 100)
    cerr `FAILED int field b\n`;
if (f.c != 200)
    cerr `FAILED int field c\n`;
cerr `ok\n`;
%%EXPECT%%
ok
%%STDIN%%
