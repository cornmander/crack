%%TEST%%
oper call on multi-function overload
%%ARGS%%
%%FILE%%
void f() { puts('f()'); }
void f(int i) { puts('f(int)'); }
void f(float g) { puts('f(float)'); }
f.oper call();
f.oper call(100);
f.oper call(1.2);
puts(typeof(f).name)
%%EXPECT%%
f()
f(int)
f(float)
Overload[.builtin.function[.builtin.void,.builtin.float], .builtin.function[.builtin.void,.builtin.int], .builtin.function[.builtin.void]]
%%STDIN%%
